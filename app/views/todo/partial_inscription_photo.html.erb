<% content_for :page_title, "To do: Plaques with photos but partial inscriptions" %>

<div class="panel">
  <h1>Plaques with photos but partial inscriptions</h1>
  <section class="admin_panel"><%= link_to("All to-do lists", "/todo") %></section>
  <%= will_paginate @plaques %>
  <% if @plaques.size > 0 %>
    <%= will_paginate @plaques %>
    <%= render @plaques %>
    <%= will_paginate @plaques %>
  <% else %>
    <p>All plaques have inscriptions. Bravo!</p>
  <% end %>
</div>